<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.travel.seoul.mapper.LikeMapper">
 	<insert id="likeinsert" parameterType="com.travel.seoul.vo.LikeVO">
 		insert into likedb(l_name, l_id) values
 		(#{l_name}, #{l_id})
 	</insert>
 	<select id="likelist" resultType="com.travel.seoul.vo.LikeVO">
 		select * from likedb	
 	</select>
 	<select id="findByID" resultType="com.travel.seoul.vo.LikeVO">
 		select * from likedb where l_id = #{l_id}
 	</select>
 	<delete id="likenameDelete" parameterType="com.travel.seoul.vo.LikeVO">
	    delete from likedb where l_name = #{l_name}
	</delete>
</mapper>