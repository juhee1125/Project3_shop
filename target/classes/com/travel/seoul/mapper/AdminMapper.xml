<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.travel.seoul.mapper.AdminMapper">
	<select id="adminlist" resultType="com.travel.seoul.vo.AdminVO">
         select * from admindb
   </select>
   <select id="getAdminByNum" resultType="com.travel.seoul.vo.AdminVO">
         select * from admindb where a_num = #{a_num}
   </select>
   <select id="getAdminID" resultType="com.travel.seoul.vo.AdminVO">
         select * from admindb where a_id = #{a_id}
   </select>
   <insert id="adminInsert" parameterType="com.travel.seoul.vo.AdminVO">
      insert into admindb(a_name,a_id,a_pw,a_phone) values( #{a_name},#{a_id},#{a_pw},#{a_phone})
   </insert>
   <delete id="adminIDDelete" parameterType="com.travel.seoul.vo.AdminVO">
      delete from admindb where a_id = #{a_id}
   </delete>
   <update id="adminUpdate" parameterType="com.travel.seoul.vo.AdminVO">
      update admindb set
      a_name=#{a_name},a_id=#{a_id}, a_pw=#{a_pw},a_phone=#{a_phone} where a_num=#{a_num}
   </update>
</mapper>

